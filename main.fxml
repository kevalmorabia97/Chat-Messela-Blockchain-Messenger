<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Label layoutX="60.0" layoutY="14.0" text="User name" />
            <TextField fx:id="user" editable="true" layoutX="14.0" layoutY="32.0" prefHeight="26.0" prefWidth="149.0" />
            <TextArea fx:id="screen" layoutX="213.0" layoutY="14.0" prefHeight="154.0" prefWidth="401.0" promptText="Send your Message here....." wrapText="true" />
            <Button fx:id="get" layoutX="70.0" layoutY="200" mnemonicParsing="false" onAction="#getKeys" text="Get Keys" />
            <Button fx:id="send" layoutX="298.0" layoutY="200.0" mnemonicParsing="false" onAction="#sendMsg" text="Send" />
            <Button fx:id="check" layoutX="425.0" layoutY="200" mnemonicParsing="false" onAction="#displayAllMsgs" text="Check all Messages" />
            <TextField fx:id="rec" layoutX="320.0" layoutY="258.0" />
            <Label layoutX="221.0" layoutY="259.0" text="Receiver:" />
      </children>
    </AnchorPane>
  </children>
</VBox>
